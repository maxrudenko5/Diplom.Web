    <section id="main">

        @Html.Partial("_SidebarLeft")

        @Html.Partial("_SidebarRight")


        <section id="content">
            <div class="container">

                @RenderBody()
            </div>
        </section>
    </section>


    @Html.Partial("_Footer");
